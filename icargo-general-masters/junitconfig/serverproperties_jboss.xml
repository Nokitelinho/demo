<?xml version="1.0" encoding="UTF-8"?>
<Server:Services xmlns:Server="http://www.ibsplc.com/ejb/serverproperties/">
    <Server:Service>
        <Server:ServerName>Weblogic</Server:ServerName>
        <Server:ServerType>Weblogic700</Server:ServerType>
        <Server:ServerId>localhost</Server:ServerId>
        <Server:ListenPort>8080</Server:ListenPort>
        <!-- For Weblogic, initial context factory class will be weblogic.jndi.WLInitialContextFactory-->
		<!-- For Websphere,initial context factory class will be com.ibm.websphere.naming.WsnInitialContextFactory-->
        <Server:InitialContextFactory>org.jboss.as.naming.InitialContextFactory</Server:InitialContextFactory>
        <!-- For Weblogic, protocol will be t3-->
		<!-- For Websphere, protocol will be iiop-->
        <Server:Protocol>remote</Server:Protocol>

    </Server:Service>

</Server:Services>
