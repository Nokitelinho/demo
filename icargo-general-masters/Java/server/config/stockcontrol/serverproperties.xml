<?xml version="1.0"?>

<!-- ServerServices -->

<Server:Services xmlns:Server="http://www.ibsplc.com/ejb/serverproperties/">

 
  <Server:Service>
    <Server:ServerName>Weblogic</Server:ServerName>
    <Server:ServerType>Weblogic700</Server:ServerType>
    <Server:ServerId>localhost</Server:ServerId>
    <Server:ListenPort>7001</Server:ListenPort>
    <Server:InitialContextFactory>weblogic.jndi.WLInitialContextFactory</Server:InitialContextFactory>
    <Server:Protocol>t3</Server:Protocol>
  </Server:Service>

</Server:Services>
