<?xml version="1.0"?>
<components screenId="uld.defaults.ucmflightexceptionlist" xmlns="http://www.ibsplc.com/icargo/components-details">

<!-- filter panels -->
	<component id="TXT_ULD_DEFAULTS_UCMFLIGHTEXCEPTIONLIST_AIRPORT" name="Airport">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_TXT_STATIONCODE_FOR_LIST</value>
			</attribute>
			
			<attribute>
				<name>type</name>
				<value>filter</value>
			</attribute>
			
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			
			<attribute>
				<name>important</name>
				<value>true</value>
			</attribute>

			
			<attribute>
				<name>tooltip</name>
				<value>uld.defaults.ucmflightexceptionlist.tooltip.airport</value>
			</attribute>
	</component>
	
		
