<?xml version="1.0"?>
<components screenId="products.defaults.catalogue"
	xmlns="http://www.ibsplc.com/icargo/components-details">
	<!--***************************Text Fields******************************-->


	<component id="BTN_PRODUCTS_DEFAULTS_CHECKAVAILABILITY_CHECK" name="Check Availability button">
				<attribute>
					<name>businesstype-id</name>
					<value>BT_BTN_SMALL</value>
				</attribute>
				<attribute>
					<name>tooltip</name>
					<value>products.defaults.Check</value>
				</attribute>

	</component>
	<component id="BTN_PRODUCTS_DEFAULTS_CHECKAVAILABILITY_CLOSE" name="Close button">
					<attribute>
						<name>businesstype-id</name>
						<value>BT_BTN_SMALL</value>
					</attribute>
					<attribute>
						<name>tooltip</name>
						<value>products.defaults.Close</value>
					</attribute>

	</component>

	<component id="TXT_PRODUCTS_DEFAULTS_CHECKAVAILABILITY_COMMODITY" name="Commodity">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_TXT_MEDIUM</value>
			</attribute>
			<attribute>
				<name>tooltip</name>
				<value>products.defaults.Commodity</value>
			</attribute>
	   			<attribute>
					<name>formatter-id</name>
					<value>FRM_RESTRICT_ALPHA</value>
	   			</attribute>
				<!--by A-5100 for ICRD-18408 starts-->
				<attribute>
					<name>mandatory</name>
					<value>true</value>
				</attribute>
				<!--by A-5100 for ICRD-18408 ends-->
	</component>

	<component id="TXT_PRODUCTS_DEFAULTS_CHECKAVAILABILITY_ORIGIN" name="Origin">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_TXT_STATIONCODE_FOR_DETAILS</value>
			</attribute>

			<attribute>
				<name>tooltip</name>
				<value>products.defaults.origin.tooltip</value>
			</attribute>
			<!--by A-5100 for ICRD-18408 starts-->
			<attribute>
					<name>mandatory</name>
					<value>true</value>
			</attribute>
			<!--by A-5100 for ICRD-18408 ends-->
	</component>

	<component id="TXT_PRODUCTS_DEFAULTS_CHECKAVAILABILITY_DESTN" name="Destination">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_TXT_STATIONCODE_FOR_DETAILS</value>
			</attribute>

			<attribute>
				<name>tooltip</name>
				<value>products.defaults.destination.tooltip</value>
			</attribute>
			<!--by A-5100 for ICRD-18408 starts-->
			<attribute>
					<name>mandatory</name>
					<value>true</value>
		   </attribute>
		   <!--by A-5100 for ICRD-18408 ends-->
	</component>

</components>