<?xml version="1.0"?>
<components screenId="mailtracking.defaults.mailbaghistory"  xmlns="http://www.ibsplc.com/icargo/components-details">

	<component id="BUT_MAILTRACKING_DEFAULTS_MBHISTORY_PRINT" name="Print button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.mbHistory.tooltip.print</value>
		</attribute>
	</component>

	<component id="BUT_MAILTRACKING_DEFAULTS_MBHISTORY_CLOSE" name="Close button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.mbHistory.tooltip.close</value>
		</attribute>
	</component>
	<component id="CMP_MAILTRACKING_DEFAULTS_MAILBAGID" name="MAILBAGID">
				<attribute>
					<name>businesstype-id</name>
					<value>BT_TXT_EXTRA_LONG</value>
				</attribute>
		      	<attribute>
					<name>styleclass-id</name>
					<value>ST_TXT_CAPS</value>
				</attribute>
				<attribute>
					<name>formatter-id</name>
					<value>FRM_RESTRICT_ALPHANUMERIC</value>
				</attribute>
				<attribute>
					<name>tooltip</name>
					<value>mailtracking.defaults.mbHistory.tooltip.mailbagid</value>
				</attribute>
	</component>
	<component id="BUT_MAILTRACKING_DEFAULTS_MBHISTORY_LIST" name="List button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.mbHistory.tooltip.list</value>
		</attribute>
	</component>
	<component id="BUT_MAILTRACKING_DEFAULTS_MBHISTORY_CLEAR" name="Clear button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.mbHistory.tooltip.clear</value>
		</attribute>
	</component>

</components>