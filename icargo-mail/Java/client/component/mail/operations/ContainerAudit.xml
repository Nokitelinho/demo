<?xml version="1.0"?>
<components screenId="mailtracking.defaults.containeraudit"  xmlns="http://www.ibsplc.com/icargo/components-details">

	<component id="CMP_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_FLIGHTNUMBER" name="Flight Number">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_FLIGHT_NUMBER</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.flightnumber</value>
		</attribute>
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_ASGPORT" name="Assign port">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_TXT_STATIONCODE_FOR_LIST</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.asgport</value>
		</attribute>
        </component>
        
        <component id="CMP_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_FLIGHTDATE" name="Flight Date">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_CALENDAR</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.flightdate</value>
		</attribute>
	</component>

        <component id="TXT_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_CONTAINERNUMBER" name="Container Number">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_TXT_MEDIUM</value>
		</attribute>

		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.containernumber</value>
		</attribute>

	</component>

	<component id="BTN_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_LIST" name="List button">
