			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.list</value>
		</attribute>
	</component>

	<component id="BTN_MAILTRACKING_DEFAULTS_CONTAINERAUDIT_CLEAR" name="Clear button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.containeraudit.tooltip.clear</value>
		</attribute>
