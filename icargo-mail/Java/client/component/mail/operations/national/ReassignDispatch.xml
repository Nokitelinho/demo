<?xml version="1.0"?>
<components screenId="mailtracking.defaults.national.reassign"  xmlns="http://www.ibsplc.com/icargo/components-details">

	<component id="BTN_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_REASSIGN" name="btnReassign">
			       	<attribute>
						<name>businesstype-id</name>
						<value>BT_BTN_SMALL</value>
					</attribute>
					<attribute>
						<name>tooltip</name>
						<value>mailtracking.defaults.national.assignMailBag.tooltip.btnReassign</value>
			       	</attribute>
			                
	</component>

	
	
	<component id="BTN_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_CLOSE" name="btnClose">
				      	<attribute>
							<name>businesstype-id</name>
							<value>BT_BUTTON_MEDIUM</value>
						</attribute>
						<attribute>
							<name>tooltip</name>
							<value>mailtracking.defaults.national.assignMailBag.tooltip.btnClose</value>
				      	</attribute>
				                
	</component>
	
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_FLIGHTNO" name="Flight No">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_FLIGHT_NUMBER</value>
			</attribute>
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			
			<attribute>
				<name>tooltip</name>
				<value>mailtracking.defaults.national.assignMailBag.tooltip.flightNo</value>
			</attribute>
	</component>

	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_FLIGHTDATE" name="Flight Date">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_CALENDAR</value>
			</attribute>
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			
			<attribute>
				<name>tooltip</name>
				<value>mailtracking.defaults.national.assignMailBag.tooltip.flightDate</value>
			</attribute>
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_POU" name="pou">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_TXT_SMALL</value>
			</attribute>
			<attribute>
				<name>lov</name>
				<value>true</value>
			</attribute>
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			<attribute>
				<name>formatter-id</name>
				<value>FRM_RESTRICT_ALPHA</value>
			</attribute>
			<attribute>
			<name>styleclass-id</name>
				<value>ST_TXT_MEDIUM_CAPS</value>
			</attribute>

			<attribute>
				<name>tooltip</name>
				<value>mailtracking.defaults.national.assignMailBag.tooltip.pou</value>
			</attribute>
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_REASSIGN_PIECES" name= "pieces">
			 <attribute>
						<name>businesstype-id</name>
						<value>BT_TXT_SMALL</value>
			</attribute>
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			<attribute>
						<name>tooltip</name>
						<value>mailtracking.defaults.national.assignMailBag.reassignpopup.tooltip.pieces</value>
			</attribute>
	</component>
	
