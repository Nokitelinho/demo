			</attribute>
			<attribute>
				<name>tooltip</name>
				<value>mailtracking.defaults.national.assignMailBag.reassignpopup.tooltip.weight</value>
			</attribute>
	</component>
	 <component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_REASSIGN_REMARKS" name="remarks">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_FREE_TEXT_DEFAULT</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.national.assignMailBag.reassignpopup.remarks</value>
		</attribute>
