<?xml version="1.0"?>
<components screenId="mailtracking.defaults.national.return"  xmlns="http://www.ibsplc.com/icargo/components-details">

	<component id="BTN_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_RETURN" name="btnReturn">
			       	<attribute>
						<name>businesstype-id</name>
						<value>BT_BUTTON_MEDIUM</value>
					</attribute>
					<attribute>
							<name>type</name>
							<value>detail</value>
					</attribute>
					<attribute>
						<name>tooltip</name>
						<value>mailtracking.defaults.national.assignMailBag.tooltip.btnReturn</value>
			        </attribute>
			                
	</component>
	
	<component id="BTN_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_CLOSE" name="btnClose">
				      	<attribute>
							<name>businesstype-id</name>
							<value>BT_BUTTON_MEDIUM</value>
						</attribute>
						<attribute>
							<name>tooltip</name>
							<value>mailtracking.defaults.national.assignMailBag.tooltip.btnClose</value>
				      	</attribute>
				                
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_REASSIGN_PIECES" name= "pieces">
			 <attribute>
						<name>businesstype-id</name>
						<value>BT_TXT_SMALL</value>
			</attribute>
			<attribute>
					<name>mandatory</name>
					<value>true</value>
			</attribute>
			<attribute>
						<name>tooltip</name>
						<value>mailtracking.defaults.national.assignMailBag.reassignpopup.tooltip.pieces</value>
			</attribute>
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNBAG_REASSIGN_WEIGHT" name= "weight">
			<attribute>
						<name>businesstype-id</name>
						<value>BT_DEFAULT_WEIGHT_ENTRY</value>
			</attribute>
			<attribute>
						<name>mandatory</name>
						<value>true</value>
			</attribute>
			<attribute>
						<name>tooltip</name>
						<value>mailtracking.defaults.national.assignMailBag.reassignpopup.tooltip.weight</value>
			</attribute>
	</component>


	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNMAILBAG_RETURNREMARKS_REMARKSCODE" name= "returnRemarksCode">
			<attribute>
				<name>businesstype-id</name>
				<value>BT_SELECT</value>
			</attribute>
			<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
			<attribute>
				<name>styleclass-id</name>
				<value>ST_CMB_MEDIUM</value>
			</attribute>
			<attribute>
				<name>tooltip</name>
				<value>mailtracking.defaults.assignMailBag.returnpopup.tooltip.remarkscode</value>
			</attribute>
	</component>
	
	<component id="CMP_MAILTRACKING_DEFAULTS_NATIONAL_ASSIGNMAILBAG_RETURNREMARKS_RETURNREMARKS" name="returnRemarks">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_FREE_TEXT_ADDRESS</value>
		</attribute>
		<attribute>
			<name>mandatory</name>
			<value>true</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.national.assignMailBag.returnpopup.tooltip.returnremarks</value>
		</attribute>
	</component>	



	

	
	
	
	
       
	
</components>