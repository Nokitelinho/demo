<?xml version="1.0"?>

<components screenId="mailtracking.defaults.offlinemailupload"  xmlns="http://www.ibsplc.com/icargo/components-details">
		   
	<component id="BTN_MAILTRACKING_DEFAULTS_UPLOADOFFLINEMAIL_SAVE" name="Save">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BUTTON</value>
		</attribute>
		<attribute>
			<name>styleclass-id</name>
			<value>ST_BTN_CLOSE</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.uploadofflinemaildetails.tooltip.save</value>
		</attribute>
	</component>
	
	<component id="BTN_MAILTRACKING_DEFAULTS_UPLOADOFFLINEMAIL_CLOSE" name="Close">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BUTTON</value>
		</attribute>
		<attribute>
			<name>styleclass-id</name>
			<value>ST_BTN_CLOSE</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.defaults.uploadofflinemaildetails.tooltip.close</value>
		</attribute>
	</component>
		   
</components>