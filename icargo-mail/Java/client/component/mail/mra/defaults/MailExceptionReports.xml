<?xml version="1.0"?>
<components screenId="mailtracking.mra.defaults.mailexceptionreports" xmlns="http://www.ibsplc.com/icargo/components-details">

	<component id="CMP_MRA_DEFAULTS_REPORT_FROMDATE" name="From Date">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_CALENDAR</value>
		</attribute>		
		<attribute>
				<name>mandatory</name>
				<value>true</value>
			</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.mra.defaults.report.tooltip.fromdate</value>
		</attribute>
	</component>
	
	<component id="CMP_MRA_DEFAULTS_REPORT_TODATE" name="To Date">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_CALENDAR</value>
		</attribute>	
<attribute>
				<name>mandatory</name>
				<value>true</value>
		</attribute>			
		<attribute> 
			<name>tooltip</name>
			<value>mailtracking.mra.defaults.report.tooltip.todate</value>
		</attribute>
	</component>

	<component id="CMP_MRA_DEFAULTS_REPORT_RPTID" name="Report Selector">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_SELECT</value>
		</attribute>
		<attribute>
			<name>styleclass-id</name>
			<value>ST_CMB_GENERAL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.mra.defaults.report.tooltip.reporttype</value>
		</attribute>
	</component>
	
	<component id="CMP_MRA_DEFAULTS_REPORT_GENERATEBTN" name="Print Button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BUTTON_LONG</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.mra.defaults.report.tooltip.btn.generate</value>
		</attribute>		
	</component>
	
	<component id="CMP_MRA_DEFAULTS_REPORT_CLOSEBTN" name="Close Button">
		<attribute>
			<name>businesstype-id</name>
			<value>BT_BTN_SMALL</value>
		</attribute>
		<attribute>
			<name>tooltip</name>
			<value>mailtracking.mra.defaults.report.btn.close</value>
		</attribute>		
	</component>

</components>