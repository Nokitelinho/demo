<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow 
	http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">


	<input name="mailFlightSummaryVO" required="true" />	
	<input name="eventCode" required="true" />

	<action-state id="performMailAWBTransactionsFlow">
		<evaluate expression="koreanMailcontroller.performMailAWBTransactionsFlow(mailFlightSummaryVO,eventCode)"/>
		<transition to="performMailAWBTransactions" />		   
	</action-state>
	<action-state id="performMailAWBTransactions">
		<evaluate expression="mAilcontroller.performMailAWBTransactions(mailFlightSummaryVO,eventCode)"/>
		<transition to="end" />		   
	</action-state>
	<end-state id="end">
		
	</end-state>
</flow>	