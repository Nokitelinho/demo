<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow 
	http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">


	<input name="mailBagVO" required="true" />	
	<input name="scanningPort" required="true" />
	
	<action-state id="saveMailUploadDetailsForAndroidFlow">
		<evaluate expression="mailUploadcontroller.saveMailUploadDetailsForAndroid(mailBagVO,scanningPort)"
		result="flowScope.scannedMailDetailsVO"/>
		<transition to="end" />		   
	</action-state>
	
	<end-state id="end">
	<output name="scannedMailDetailsVO" />
	</end-state>
</flow>	