<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:isf="http://www.ibsplc.com/schema/IBSServiceFlow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.ibsplc.com/schema/IBSServiceFlow
           http://www.ibsplc.com/schema/IBSServiceFlow/IBSServiceFlow-1.0.xsd"> 
	
	
	<isf:service id="mailMraFlowServices"
		interface="com.ibsplc.icargo.business.mail.mra.MailTrackingMRABI">	
		<isf:interceptorNames>
			<value>loggingInterceptor</value> 
			<value>sproutExceptionTranslator</value> 
		</isf:interceptorNames>
 	</isf:service>	 
	<isf:service id="mailOperationsFlowServices"
		interface="com.ibsplc.icargo.business.mail.operations.MailTrackingDefaultsBI">	
		<isf:interceptorNames>
			<value>loggingInterceptor</value> 
			<value>sproutExceptionTranslator</value> 
		</isf:interceptorNames>
 	</isf:service>	
</beans>